generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User_info {
  user_id Int @id
  first_name String
  last_name String
  age Int
  gender String
  birth_date  DateTime
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model User_credentials {
  user_id Int @id
  email String
  password_ String
}
